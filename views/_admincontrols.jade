.admin-controls(class=replace ? "noanim" : "")
	ul.admin-users-list.close
		each user in users
			if !user.disconnected
				li.user(data-user-id=user.userId)
					= user.userName
					if user.userId != userId
						.caret.fa.fa-caret-right
						.tooltip
							.js-boot.fa.fa-user-times
							.js-rename.fa.fa-edit
							if user.admin
								.js-disadminify.fa.fa-lock
							else 
								.js-adminify.fa.fa-unlock
							.js-ban.fa.fa-unlink
	.js-admin-users.fa.fa-users.admin-control
	.js-admin-next.fa.fa-step-forward.admin-control
	.js-admin-shuffle.fa.fa-random.admin-control
	.js-admin-stop.fa.fa-stop.admin-control